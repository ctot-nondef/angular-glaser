{
  "title[type=alt]":{
    "wrapElement":{
      "tag":"div",
      "attributes":{
        "layout":"row",
        "layout-align":"start center"
      }
    },
    "insertElement":{
      "position":"beforebegin",
      "markup":"<h3 class=\"md-subheader\"><md-icon class=\"panelicon\">bookmark</md-icon>Alternative Title:</h3>"
    }
  },
  "text":{
    "replaceElement":"md-tabs",
    "setAttribute":{
      "md-dynamic-height":"",
      "md-border-bottom":"",
      "md-selected":"0",
      "md-stretch-tabs":"always",
      "style":"width: 100%;"
    },
    "insertElement":{
      "position":"beforebegin",
      "markup":"<div layout-padding></div>"
    }
  },
  "div[type=edition]":{
    "replaceElement":"md-tab",
    "setAttribute":{
      "label":"Edition"
    }
  },
  "div[type=translation]":{
    "replaceElement":"md-tab",
    "setAttribute":{
      "label":"Translation"
    }
  },
  "div[type=bibliography]":{
    "replaceElement":"md-tab",
    "setAttribute":{
      "label":"Bibliography"
    }
  },
  "*|lb":{
    "replaceElement":"br",
    "setAttribute":{
    }
  }
}
