[
  {
    "selector":"teiHeader",
    "actions":[
        {
          "setAttribute":{
            "md-component-id":"teiHeader",
            "layout-padding":""
          }
        },
        {
          "wrapContent":{
            "tag":"md-expansion-panel-content"
          }
        },
        {
          "insertElement":{
            "position":"afterbegin",
            "markup":"<md-expansion-panel-header><md-icon class=\"panelicon\">settings_ethernet</md-icon><div class=\"md-title\">TEI Header Information</div><div class=\"md-summary\">Expanded Summary</div><md-expansion-panel-icon></md-expansion-panel-icon></md-expansion-panel-header>"
          }
        },
        {
          "insertElement":{
            "position":"beforeend",
            "markup":"<md-expansion-panel-footer><div flex></div><md-button class=\"md-warn\" ng-click=\"$panel.collapse()\">Collapse</md-button></md-expansion-panel-footer>"
          }
        },
        {
          "wrapContent":{
            "tag":"md-expansion-panel-expanded"
          }
        },
        {
          "insertElement":{
            "position":"afterbegin",
            "markup":"<md-expansion-panel-collapsed><md-icon class=\"panelicon\">settings_ethernet</md-icon><div class=\"md-title\">TEI Header Information</div><div class=\"md-summary\">Summary</div><md-expansion-panel-icon></md-expansion-panel-icon></md-expansion-panel-collapsed>"
          }
        },
        {
          "replaceElement":"md-expansion-panel"
        }
    ]
  },
